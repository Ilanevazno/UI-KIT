[32m+[m[32m/* eslint-disable global-require */[m
[32m+[m[32mimport 'leaflet';[m
[32m+[m[32mimport 'leaflet-css';[m
[32m+[m
[32m+[m[32mclass Location {[m
[32m+[m[32m  constructor(mapContainer) {[m
[32m+[m[32m    this.$htmlContainer = $(mapContainer);[m
[32m+[m[32m    this.coords = JSON.parse(this.$htmlContainer.attr('data-coords'));[m
[32m+[m[32m    this.mymap = L.map(mapContainer).setView(this.coords, 13);[m
[32m+[m[32m    this.mapIcon = require('./img/marker-icon.png');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  bindActions() {[m
[32m+[m[32m    const $mapContainer = $(this.mymap._container);[m
[32m+[m[32m    const $showCurrentPosition = $mapContainer.find('.js-overlay-btn-set-address');[m
[32m+[m[32m    const $findMarker = $mapContainer.find('.js-overlay-btn-find-marker');[m
[32m+[m[32m    $showCurrentPosition.on('click.setLocationPosition', this.setNewAddress.bind(this));[m
[32m+[m[32m    $findMarker.on('click.findLocationMarker', this.showCurrentPosition.bind(this));[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  showCurrentPosition() {[m
[32m+[m[32m    this.mymap.panTo(this.coords);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  setNewAddress() {[m
[32m+[m[32m    const requestLatitude = prompt('–í–≤–µ–¥–∏—Ç–µ –¥–æ–ª–≥–æ—Ç—É');[m
[32m+[m[32m    const requestLongitude = prompt('–í–≤–µ–¥–∏—Ç–µ —à–∏—Ä–∏–Ω—É');[m
[32m+[m[32m    if (requestLatitude && requestLongitude) {[m
[32m+[m[32m      this.mymap.setView([requestLatitude, requestLongitude], 13);[m
[32m+[m[32m      const placeMark = L.icon({[m
[32m+[m[32m        iconUrl: this.mapIcon,[m
[32m+[m[32m        iconSize: [40, 56],[m
[32m+[m[32m        iconAnchor: [22, 94],[m
[32m+[m[32m        popupAnchor: [-3, -76],[m
[32m+[m[32m        shadowUrl: '',[m
[32m+[m[32m        shadowSize: [68, 95],[m
[32m+[m[32m        shadowAnchor: [22, 94],[m
[32m+[m[32m      });[m
[32m+[m[32m      L.marker([requestLatitude, requestLongitude], { icon: placeMark }).addTo(this.mymap);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
