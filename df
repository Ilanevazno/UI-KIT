[32m+[m[32m      maxZoom: 18,[m
[32m+[m[32m      id: 'mapbox/streets-v11',[m
[32m+[m[32m      tileSize: 512,[m
[32m+[m[32m      zoomOffset: -1,[m
[32m+[m[32m      accessToken: 'pk.eyJ1IjoiaWxhbmV2YXpubzExIiwiYSI6ImNrNndsbGR6ZjBkMWUzaG1vZHp0dWZmNGoifQ.hdk2ziw77v-cKLyCc4ghoA',[m
[32m+[m[32m    }).addTo(this.mymap);[m
[32m+[m
[32m+[m
[32m+[m[32m    const placeMark = L.icon({[m
[32m+[m[32m      iconUrl: this.mapIcon,[m
[32m+[m[32m      iconSize: [40, 56],[m
[32m+[m[32m      iconAnchor: [22, 94],[m
[32m+[m[32m      popupAnchor: [-3, -76],[m
[32m+[m[32m      shadowUrl: '',[m
[32m+[m[32m      shadowSize: [68, 95],[m
[32m+[m[32m      shadowAnchor: [22, 94],[m
[32m+[m[32m    });[m
[32m+[m[32m    L.marker(this.coords, { icon: placeMark }).addTo(this.mymap);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  bootstrap() {[m
[32m+[m[32m    this.render();[m
[32m+[m[32m    this.bindActions();[m
[32m+[m[32m  }[m
