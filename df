[32m+[m[32mclass Stages {[m
[32m+[m[32m  constructor(selector, activeStage) {[m
[32m+[m[32m    this.$htmlContainer = $(selector);[m
[32m+[m[32m    this.$stagesItems = this.$htmlContainer.find('.widget-stages').find('ul').find('li');[m
[32m+[m[32m    this.activeStage = activeStage || 1;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  clearStages() {[m
[32m+[m[32m    this.$stagesItems.each((idx, itm) => {[m
[32m+[m[32m      $(itm).removeClass('is-active');[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  setStage(current) {[m
[32m+[m[32m    $(this.$stagesItems).eq(current).addClass('is-active');[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport default Stages;[m
