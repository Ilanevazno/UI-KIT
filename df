[32m+[m[32mclass Dropdown {[m
[32m+[m[32m  constructor(selector) {[m
[32m+[m[32m    this.$htmlContainer = $(selector);[m
[32m+[m[32m    this.$htmlTextArea = this.$htmlContainer.find('.js-dropdown-textarea');[m
[32m+[m[32m    this.$htmlInnerTextArea = this.$htmlContainer.find('.js-dropdown-text-inner');[m
[32m+[m[32m    this.$menu = this.$htmlContainer.find('.js-dropdown-menu');[m
[32m+[m[32m    this.currentState = null;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  bindActions() {[m
[32m+[m[32m    this.$htmlContainer.find('.js-dropdown-textarea').on('click.dopdown-textarea keypress', this.toggleMenuState.bind(this));[m
[32m+[m[32m    this.$htmlContainer.find('.js-dropdown-label-item').on('click.dropdown-label', this.chooseMenuItem.bind(this));[m
[32m+[m[32m    $(document).on('click.closeDropdown', this.closeMenu.bind(this));[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  closeMenu(e) {[m
[32m+[m[32m    const $catchedTarget = $(e.target);[m
[32m+[m
[32m+[m[32m    const checkTarget = () => {[m
[32m+[m[32m      if (this.$menu.is('.widget-select__label_open') && (this.$htmlContainer.find($catchedTarget).length === 0)) {[m
[32m+[m[32m        return true;[m
[32m+[m[32m      }[m
[32m+[m[32m      return false;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    const canClose = checkTarget();[m
[32m+[m
[32m+[m[32m    if (canClose) {[m
[32m+[m[32m      this.$menu.toggleClass('widget-select__label_open');[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  toggleMenuState() {[m
[32m+[m[32m    this.$menu.toggleClass('widget-select__label_open');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  chooseMenuItem(event) {[m
[32m+[m[32m    const targetText = $(event.target).html();[m
[32m+[m[32m    this.$htmlInnerTextArea.text(targetText);[m
[32m+[m[32m    this.currentState = targetText;[m
[32m+[m[32m    this.toggleMenuState();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  getCurrentState() {[m
[32m+[m[32m    return this.currentState;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  bootstrap() {[m
[32m+[m[32m    this.bindActions();[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport default Dropdown;[m
