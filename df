[32m+[m[32mclass Search {[m
[32m+[m[32m  constructor(element) {[m
[32m+[m[32m    this.$htmlElement = $(document).find(element);[m
[32m+[m[32m    this.htmlItemList = {[m
[32m+[m[32m      $searchItems: this.$htmlElement.find('.form-search__items'),[m
[32m+[m[32m      $searchBtn: this.$htmlElement.find('.form-search__btn-find'),[m
[32m+[m[32m      $inputLabel: this.$htmlElement.find('.form-search__str'),[m
[32m+[m[32m      $errorNotify: this.$htmlElement.find('.form-search__str-status'),[m
[32m+[m[32m    };[m
[32m+[m[32m    this.openMM = false;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  bindActions() {[m
[32m+[m[32m    this.htmlItemList.$inputLabel.on('click.searchLabel', this.prepareLine.bind(this));[m
[32m+[m[32m    this.htmlItemList.$searchItems.find('li').on('click.searchItem', this.selectFindedOption.bind(this));[m
[32m+[m[32m    this.htmlItemList.$inputLabel.on('input.searchForm', this.startSearch.bind(this));[m
[32m+[m[32m    this.htmlItemList.$searchBtn.on('click.searchButton', this.pressSearchButton.bind(this));[m
[32m+[m[32m    this.htmlItemList.$errorNotify.on('click.errorNotify', this.prepareLine.bind(this));[m
[32m+[m[32m    $(document).on('click.closeSearch', this.closeMenu.bind(this));[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  closeMenu(e) {[m
[32m+[m[32m    const $catchedTarget = $(e.target);[m
[32m+[m
[32m+[m[32m    const checkTarget = () => {[m
[32m+[m[32m      if (!this.htmlItemList.$searchItems.is('.form-search__hidden-item') && (this.$htmlElement.find($catchedTarget).length === 0)) {[m
[32m+[m[32m        return true;[m
[32m+[m[32m      }[m
[32m+[m[32m      return false;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    const canClose = checkTarget();[m
[32m+[m
[32m+[m[32m    if (canClose) {[m
[32m+[m[32m      this.htmlItemList.$searchItems.toggleClass('form-search__hidden-item');[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  prepareLine() {[m
[32m+[m[32m    this.htmlItemList.$searchItems.toggleClass('form-search__hidden-item');[m
[32m+[m[32m    this.htmlItemList.$errorNotify.css('display', 'none');[m
[32m+[m[32m    this.openMM = true;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  startSearch(e) {[m
[32m+[m[32m    const val = e.target.value.trim();[m
[32m+[m[32m    const itemList = this.htmlItemList.$searchItems.find('li');[m
[32m+[m
[32m+[m[32m    this.htmlItemList.$searchItems.removeClass('form-search__hidden-item');[m
[32m+[m
[32m+[m[32m    if (val !== '') {[m
[32m+[m[32m      itemList.each((idx, elem) => {[m
[32m+[m[32m        if (elem.innerText.search(val) === -1) {[m
[32m+[m[32m          $(elem).hide();[m
[32m+[m[32m        } else {[m
[32m+[m[32m          $(elem).show();[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m    } else {[m
[32m+[m[32m      itemList.each((idx, elem) => {[m
[32m+[m[32m        $(elem).show();[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  pressSearchButton() {[m
[32m+[m[32m    const itemList = this.htmlItemList.$searchItems.find('li');[m
[32m+[m
[32m+[m[32m    const findingElement = [...itemList].some([m
[32m+[m[32m      (element) => element.innerText === this.htmlItemList.$inputLabel.val(),[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    this.htmlItemList.$searchBtn.removeClass('form-search__btn-find_check_valid form-search__btn-find_check_invalid');[m
[32m+[m
[32m+[m[32m    if (findingElement) {[m
[32m+[m[32m      this.htmlItemList.$searchBtn.addClass('form-search__btn-find_check_valid');[m
[32m+[m[32m      this.htmlItemList.$errorNotify.css('display', 'none');[m
[32m+[m[32m    } else {[m
[32m+[m[32m      this.htmlItemList.$searchBtn.addClass('form-search__btn-find_check_invalid');[m
[32m+[m[32m      this.htmlItemList.$errorNotify.css('display', 'inline-flex');[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  selectFindedOption(e) {[m
[32m+[m[32m    this.htmlItemList.$inputLabel.val($(e.target).text());[m
[32m+[m[32m    this.htmlItemList.$searchItems.toggleClass('form-search__hidden-item');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  bootstrap() {[m
[32m+[m[32m    this.bindActions();[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport default Search;[m
