include ../arrows/arrows
mixin arrows-section (title)
  section.arrows-section
    h2.arrows-section__title= title
    .arrows-section__container
      +arrow({ direction: "arrow-left", isDisable: false, })
      +arrow({ direction: "arrow-right", isDisable: false, })
      +arrow({ direction: "arrow-left", isDisable: true, })
      +arrow({ direction: "arrow-right", isDisable: true, })
      +arrow({ direction: "arrow-left", isDisable: false, })
      +arrow({ direction: "arrow-right", isDisable: false })