mixin stages(data = {})
  -
    const {
      maxStep = '',
      currentStep = '',
    } = data;
  section.stages
    ul.stages__multi-steps
      - for (var i = 0; i < maxStep; i += 1)
      - if ((i + 1) === currentStep)
        li.stages__multi-steps-item.is-active
      - else
        li.stages__multi-steps-item