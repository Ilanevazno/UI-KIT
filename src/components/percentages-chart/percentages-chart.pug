mixin percentages-chart(currentFill = 0)
  - const maxCount = 100;
  section.percentages-chart
    svg.percentages-chart__figure(width="100%" height="100%" viewBox="0 0 42 42")
      circle.percentages-chart__filled(
        cx="21"
        cy="21"
        r="15.91549430918954"
        fill="transparent"
        stroke="#ce4b99"
        stroke-width="2"
        stroke-dasharray=`${currentFill} ${maxCount - currentFill}`
        stroke-dashoffset="25")
        .percentages-chart__text= currentFill