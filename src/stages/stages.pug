mixin stages(maxStep)
    div(class="stage")
        input(type="text" data-provide="slider" data-slider-min="1" data-slider-max="5" data-slider-step="1" data-slider-value="3" data-slider-ticks= ''+maxStep data-slider-ticks-labels= ''+maxStep data-slider-tooltip="hide" data-slider-id="st")

+stages('[1,2,3,4,5]')